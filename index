    <!-- Chatbot "O Conselheiro" -->
    <aside id="chatbot-container" class="chatbot-container" aria-live="polite">
        <button id="chatbot-toggle" class="chatbot-toggle" aria-label="Abrir O Conselheiro StrateUp" aria-expanded="false"><i class="fas fa-brain"></i></button>
        <div id="chatbot-widget" class="chatbot-widget" role="dialog" aria-modal="true" aria-labelledby="chatbot-title">
            <div class="chatbot-header">
                <h3 id="chatbot-title">Conselheiro StrateUp</h3>
                <button id="chatbot-close" class="chatbot-close" aria-label="Fechar Chatbot"><i class="fas fa-times"></i></button>
            </div>
            <div class="chatbot-messages" id="chatbot-messages" aria-live="polite">
                <!-- Mensagens serão inseridas aqui via JS -->
            </div>
            <div class="chatbot-input-container">
                <input type="text" id="chatbot-input" placeholder="Digite sua dúvida..." aria-label="Digite sua mensagem">
                <button id="chatbot-send" aria-label="Enviar mensagem"><i class="fas fa-paper-plane"></i></button>
            </div>
        </div>
    </aside>
    
    <!-- Lead Capture Popup (DotCom Secrets - Exit Intent) -->
    <div class="lead-popup" id="lead-popup">
        <div class="lead-popup-content">
            <button class="lead-popup-close" id="lead-popup-close"><i class="fas fa-times"></i></button>
            <h3 class="lead-popup-title">Espere! Antes de sair...</h3>
            <div class="lead-popup-subtitle">Quer aumentar suas conversões em 30 dias?</div>
            <p>Receba nosso guia exclusivo com 7 táticas de otimização de funil que você pode implementar <strong>hoje mesmo</strong> para ver resultados em 30 dias ou menos.</p>
            <form class="lead-popup-form" id="lead-popup-form">
                <!-- Campos ocultos para rastreamento (DotCom Secrets) -->
                <input type="hidden" name="utm_source" id="hidden_utm_source">
                <input type="hidden" name="utm_medium" id="hidden_utm_medium">
                <input type="hidden" name="utm_campaign" id="hidden_utm_campaign">
                <input type="hidden" name="lead_temp" id="hidden_lead_temp" value="warm">
                <input type="hidden" name="lead_origin" id="hidden_lead_origin" value="exit_intent">
                <input type="hidden" name="page_url" id="hidden_page_url">
                <input type="hidden" name="referrer" id="hidden_referrer">
                
                <!-- Campos visíveis -->
                <input type="text" name="name" placeholder="Seu nome" required>
                <input type="email" name="email" placeholder="Seu melhor email" required>
                <button type="submit">QUERO RECEBER O GUIA</button>
            </form>
            <p class="lead-popup-privacy">Seus dados estão seguros. Não enviamos spam.</p>
        </div>
    </div>
    
    <!-- Campos ocultos para rastreamento (DotCom Secrets) -->
    <div class="lead-origin-tracker">
        <input type="hidden" id="visit_time" value="">
        <input type="hidden" id="page_url" value="">
        <input type="hidden" id="referrer" value="">
        <input type="hidden" id="lead_temp" value="warm">
    </div>

    <!-- Scripts -->
    <script>
        document.addEventListener('DOMContentLoaded', function() {
            // Atualizar ano no footer
            document.getElementById('current-year').textContent = new Date().getFullYear();
            
            // Rastreamento de lead (DotCom Secrets)
            function trackLeadOrigin() {
                // Armazenar data e hora da visita
                document.getElementById('visit_time').value = new Date().toString();
                
                // Armazenar URL atual
                document.getElementById('page_url').value = window.location.href;
                document.getElementById('hidden_page_url').value = window.location.href;
                
                // Armazenar referenciador
                const referrer = document.referrer;
                document.getElementById('referrer').value = referrer;
                document.getElementById('hidden_referrer').value = referrer;
                
                // Capturar parâmetros UTM
                const urlParams = new URLSearchParams(window.location.search);
                const utmSource = urlParams.get('utm_source');
                const utmMedium = urlParams.get('utm_medium');
                const utmCampaign = urlParams.get('utm_campaign');
                
                if (utmSource) document.getElementById('hidden_utm_source').value = utmSource;
                if (utmMedium) document.getElementById('hidden_utm_medium').value = utmMedium;
                if (utmCampaign) document.getElementById('hidden_utm_campaign').value = utmCampaign;
                
                // Armazenar em cookies para persistência
                if (utmSource) setCookie('utm_source', utmSource, 30);
                if (utmMedium) setCookie('utm_medium', utmMedium, 30);
                if (utmCampaign) setCookie('utm_campaign', utmCampaign, 30);
                if (referrer) setCookie('referrer', referrer, 30);
            }
            
            // Função para definir cookies
            function setCookie(name, value, days) {
                let expires = '';
                if (days) {
                    const date = new Date();
                    date.setTime(date.getTime() + (days * 24 * 60 * 60 * 1000));
                    expires = '; expires=' + date.toUTCString();
                }
                document.cookie = name + '=' + (value || '') + expires + '; path=/';
            }
            
            // Função para alternar o menu mobile
            const mobileMenuBtn = document.getElementById('mobile-menu-btn');
            const nav = document.querySelector('.nav');
            
            if (mobileMenuBtn) {
                mobileMenuBtn.addEventListener('click', function() {
                    nav.classList.toggle('active');
                    const isExpanded = mobileMenuBtn.getAttribute('aria-expanded') === 'true';
                    mobileMenuBtn.setAttribute('aria-expanded', !isExpanded);
                    mobileMenuBtn.innerHTML = isExpanded ? '<i class="fas fa-bars"></i>' : '<i class="fas fa-times"></i>';
                    document.body.classList.toggle('no-scroll');
                });
            }
            
            // Efeito de palavras cíclicas no hero
            const cyclingWords = ['em Clientes?', 'em Vendas?', 'em Resultados?'];
            let currentWordIndex = 0;
            const cyclingWordElement = document.getElementById('cycling-word');
            
            if (cyclingWordElement) {
                setInterval(() => {
                    cyclingWordElement.classList.add('fade-out');
                    
                    setTimeout(() => {
                        currentWordIndex = (currentWordIndex + 1) % cyclingWords.length;
                        cyclingWordElement.textContent = cyclingWords[currentWordIndex];
                        cyclingWordElement.classList.remove('fade-out');
                        cyclingWordElement.classList.add('fade-in');
                        
                        setTimeout(() => {
                            cyclingWordElement.classList.remove('fade-in');
                        }, 500);
                    }, 500);
                }, 4000);
            }
            
            // Animação de h2 ao scroll
            const animatedTitles = document.querySelectorAll('.animated-title');
            
            if (animatedTitles.length) {
                // Quebrar cada título em palavras
                animatedTitles.forEach(title => {
                    const text = title.textContent;
                    const words = text.split(' ');
                    title.textContent = '';
                    
                    words.forEach(word => {
                        const span = document.createElement('span');
                        span.textContent = word + ' ';
                        span.className = 'h-word';
                        title.appendChild(span);
                    });
                });
                
                // Observar quando os títulos entram na viewport
                const observer = new IntersectionObserver((entries) => {
                    entries.forEach(entry => {
                        if (entry.isIntersecting) {
                            entry.target.classList.add('animate-title');
                        }
                    });
                }, { threshold: 0.1 });
                
                animatedTitles.forEach(title => {
                    observer.observe(title);
                });
            }
            
            // Funcionalidade das abas (Bloqueios Mentais)
            const blockTabs = document.querySelectorAll('.block-tab');
            const blockContents = document.querySelectorAll('.block-content');
            
            if (blockTabs.length && blockContents.length) {
                blockTabs.forEach(tab => {
                    tab.addEventListener('click', () => {
                        const tabId = tab.getAttribute('data-tab');
                        
                        // Desativar todas as abas
                        blockTabs.forEach(t => {
                            t.classList.remove('active');
                            t.setAttribute('aria-selected', 'false');
                            t.setAttribute('tabindex', '-1');
                        });
                        
                        // Esconder todos os conteúdos
                        blockContents.forEach(content => {
                            content.classList.remove('active');
                            content.setAttribute('hidden', 'true');
                        });
                        
                        // Ativar a aba clicada
                        tab.classList.add('active');
                        tab.setAttribute('aria-selected', 'true');
                        tab.removeAttribute('tabindex');
                        
                        // Mostrar o conteúdo correspondente
                        const activeContent = document.querySelector(`.block-content[data-content="${tabId}"]`);
                        if (activeContent) {
                            activeContent.classList.add('active');
                            activeContent.removeAttribute('hidden');
                        }
                    });
                });
                
                // Navegação por teclado nas abas
                blockTabs.forEach(tab => {
                    tab.addEventListener('keydown', (e) => {
                        const tabs = Array.from(blockTabs);
                        const index = tabs.indexOf(tab);
                        let nextTab;
                        
                        switch (e.key) {
                            case 'ArrowRight':
                            case 'ArrowDown':
                                nextTab = tabs[(index + 1) % tabs.length];
                                break;
                            case 'ArrowLeft':
                            case 'ArrowUp':
                                nextTab = tabs[(index - 1 + tabs.length) % tabs.length];
                                break;
                            case 'Home':
                                nextTab = tabs[0];
                                break;
                            case 'End':
                                nextTab = tabs[tabs.length - 1];
                                break;
                            default:
                                return;
                        }
                        
                        nextTab.click();
                        nextTab.focus();
                        e.preventDefault();
                    });
                });
            }
            
            // Chatbot funcionalidade
            const chatbotToggle = document.getElementById('chatbot-toggle');
            const chatbotWidget = document.getElementById('chatbot-widget');
            const chatbotClose = document.getElementById('chatbot-close');
            const chatbotMessages = document.getElementById('chatbot-messages');
            const chatbotInput = document.getElementById('chatbot-input');
            const chatbotSend = document.getElementById('chatbot-send');
            
            if (chatbotToggle && chatbotWidget) {
                // Abrir/fechar chatbot
                chatbotToggle.addEventListener('click', () => {
                    chatbotWidget.classList.toggle('active');
                    const isExpanded = chatbotToggle.getAttribute('aria-expanded') === 'true';
                    chatbotToggle.setAttribute('aria-expanded', !isExpanded);
                    
                    if (!isExpanded) {
                        // Adicionar mensagem inicial do bot quando o chatbot é aberto
                        if (chatbotMessages.children.length === 0) {
                            addBotMessage('Olá! Sou o Conselheiro StrateUp. Como posso ajudar você a otimizar seu funil de vendas hoje?');
                            
                            // Simular está digitando
                            setTimeout(() => {
                                showTypingIndicator();
                                
                                setTimeout(() => {
                                    hideTypingIndicator();
                                    addBotMessage('Você gostaria de saber mais sobre nossa metodologia de otimização ou ver alguns casos de sucesso?');
                                }, 2000);
                            }, 1000);
                        }
                        
                        chatbotInput.focus();
                    }
                });
                
                // Fechar chatbot
                if (chatbotClose) {
                    chatbotClose.addEventListener('click', () => {
                        chatbotWidget.classList.remove('active');
                        chatbotToggle.setAttribute('aria-expanded', 'false');
                    });
                }
                
                // Enviar mensagem
                function sendMessage() {
                    const message = chatbotInput.value.trim();
                    if (message) {
                        addUserMessage(message);
                        chatbotInput.value = '';
                        
                        // Simular resposta do bot
                        setTimeout(() => {
                            showTypingIndicator();
                            
                            setTimeout(() => {
                                hideTypingIndicator();
                                
                                // Respostas predefinidas
                                if (message.toLowerCase().includes('preço') || message.toLowerCase().includes('valor') || message.toLowerCase().includes('custo')) {
                                    addBotMessage('Nossa análise estratégica de funil é oferecida gratuitamente para empresas qualificadas. Após a análise, apresentamos um plano personalizado com opções que se adequam ao seu orçamento. Você gostaria de agendar sua análise gratuita?');
                                } else if (message.toLowerCase().includes('tempo') || message.toLowerCase().includes('prazo')) {
                                    addBotMessage('O processo de otimização varia conforme a complexidade do seu funil, mas geralmente vemos os primeiros resultados em 14-30 dias. Você já tem um funil de vendas estruturado?');
                                } else {
                                    addBotMessage('Obrigado pela sua mensagem. Um de nossos especialistas entrará em contato em breve para discutir como podemos ajudar a otimizar seu funil de vendas. Enquanto isso, gostaria de agendar uma análise gratuita?');
                                }
                            }, 1500);
                        }, 500);
                    }
                }
                
                if (chatbotSend) {
                    chatbotSend.addEventListener('click', sendMessage);
                }
                
                if (chatbotInput) {
                    chatbotInput.addEventListener('keypress', (e) => {
                        if (e.key === 'Enter') {
                            sendMessage();
                        }
                    });
                }
                
                // Funções auxiliares para o chatbot
                function addBotMessage(message) {
                    const messageDiv = document.createElement('div');
                    messageDiv.className = 'chatbot-message bot';
                    messageDiv.textContent = message;
                    chatbotMessages.appendChild(messageDiv);
                    chatbotMessages.scrollTop = chatbotMessages.scrollHeight;
                }
                
                function addUserMessage(message) {
                    const messageDiv = document.createElement('div');
                    messageDiv.className = 'chatbot-message user';
                    messageDiv.textContent = message;
                    chatbotMessages.appendChild(messageDiv);
                    chatbotMessages.scrollTop = chatbotMessages.scrollHeight;
                }
                
                function showTypingIndicator() {
                    const typingDiv = document.createElement('div');
                    typingDiv.className = 'typing-indicator';
                    typingDiv.id = 'typing-indicator';
                    typingDiv.innerHTML = '<span></span><span></span><span></span>';
                    chatbotMessages.appendChild(typingDiv);
                    chatbotMessages.scrollTop = chatbotMessages.scrollHeight;
                }
                
                function hideTypingIndicator() {
                    const typingIndicator = document.getElementById('typing-indicator');
                    if (typingIndicator) {
                        typingIndicator.remove();
                    }
                }
            }
            
            // Lead popup (exit intent)
            const leadPopup = document.getElementById('lead-popup');
            const leadPopupClose = document.getElementById('lead-popup-close');
            const leadPopupForm = document.getElementById('lead-popup-form');
            
            if (leadPopup && leadPopupClose) {
                // Fechar popup
                leadPopupClose.addEventListener('click', () => {
                    leadPopup.classList.remove('active');
                });
                
                // Detectar intenção de saída
                document.addEventListener('mouseleave', (e) => {
                    // Se o mouse sair pelo topo da página e o popup não tiver sido mostrado antes
                    if (e.clientY < 0 && !sessionStorage.getItem('popupShown')) {
                        leadPopup.classList.add('active');
                        sessionStorage.setItem('popupShown', 'true');
                    }
                });
                
                // Também mostrar após tempo determinado
                setTimeout(() => {
                    if (!sessionStorage.getItem('popupShown')) {
                        leadPopup.classList.add('active');
                        sessionStorage.setItem('popupShown', 'true');
                    }
                }, 30000); // 30 segundos
                
                // Submissão do formulário
                if (leadPopupForm) {
                    leadPopupForm.addEventListener('submit', (e) => {
                        e.preventDefault();
                        // Aqui seria a integração com sistema de email
                        alert('Obrigado! Seu guia foi enviado para seu email.');
                        leadPopup.classList.remove('active');
                    });
                }
            }
            
            // Simulação de clique no vídeo
            const videoPlaceholder = document.querySelector('.video-placeholder');
            if (videoPlaceholder) {
                videoPlaceholder.addEventListener('click', () => {
                    alert('O vídeo seria reproduzido aqui em uma implementação completa.');
                });
            }
            
            // Iniciar rastreamento
            trackLeadOrigin();
        });
    </script>
</body>
</html>
